<div class= "container">
{{#each messages}}
<div class="alert alert-danger">{{this}}</div>
{{/each}}
<h1>List User</h1>
{{# each users}}
        {{# each this}}
          {{#if name}}
            <div class="col-sm-6 col-md-4">
                <div class="thumbnail">
                    <div class="caption">
                        <h3>{{this.name}}</h3>
                        <p>{{this.email}}</p>
                    </div>
                     {{#if this.admin}}
                         <img class="" src="https://thumbs.dreamstime.com/b/ic%C3%B4ne-de-vecteur-deux-admin-couleur-concept-strat%C3%A9gie-le-symbole-bleu-d-isolement-signe-peut-%C3%AAtre-utilisation-pour-web-mobile-149167982.jpg">  
                        <p>Admin</p>
                        <a href="/admin/deletead/{{this._id}}" class="btn btn-success" role="button">Delete admin</a>
                    {{/if}}
                    {{#if this.vendor}}
                        <img class="" src="data:image/jpeg;base64,">     
                        <p>Vendor</p>
                        <a href="/admin/deletevendor/{{this._id}}" class="btn btn-success" role="button">Delete Vendor</a>
                    {{/if}}
                    {{#if this.user}}
                        <img class="" src="https://toppng.com/uploads/preview/user-font-awesome-nuevo-usuario-icono-11563566658mjtfvilgcs.png">  
                        <p>User</p>
                        <a href="/admin/addad/{{this._id}}" class="btn btn-success" role="button">Add admin</a>
                        <a href="/admin/addvendor/{{this._id}}" class="btn btn-success" role="button">Add vendor</a>
                    {{/if}}
                         
                </div>
            </div>
            {{/if}}
        {{/each}}
{{/each}}
</div>